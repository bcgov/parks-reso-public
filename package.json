    ]
  },
  "dependencies": {
    "axios": "^1.4.0",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.1",
    "luxon": "^3.2.1",
    "node-jose": "^2.2.0",
    "svg-captcha": "^1.4.0",
    "winston": "^3.8.0",
    "write-excel-file": "^1.3.16"
