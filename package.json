{
  "name": "bcparks-ar-api",
  "version": "1.0.0",
  "description": "BCParks Attendance and Revenue Backend",
  "devDependencies": {
    "@digitalspace/dynamodb-migrate": "^1.0.6",
    "aws-sdk-mock": "^5.4.0",
    "jest": "^29.5.0",
    "read-excel-file": "^5.3.4",
    "serverless": "^3.18.1",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-include-dependencies": "^5.0.0"
